{
    "version": 2,
    "public": false,
    "builds": [
      { "src": "package.json", "use": "@now/static-build" },
      { "src": "./server/index.js", "use": "@now/node-server" }
    ],
    "routes": [
        
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "index.html/" }
    ]
}