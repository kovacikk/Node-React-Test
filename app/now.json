{
    "version": 2,
    "public": false,
    "builds": [
      { "src": "package.json", "use": "@now/node-server" },
      { "src": "./server/index.js", "use": "@now/node-server" }
    ],
    "routes": [
        { "src": "/server/.*", "dest": "/server/index.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "index.html" }
    ]
}